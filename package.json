{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "start-clean": "turbo run build && turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
  "build:web": "npm run build --workspace=apps/web",
  "build:desktop": "npm run build --workspace=apps/desktop",
  "build:eleventy": "eleventy --input=src --output=public",
  "dev:eleventy": "eleventy --serve --input=src --output=public",
  "build:static": "npm run build:eleventy && npm run build:web && npm run build:desktop && node scripts/assemble_static.js",
  "build:static:clean": "./scripts/build_static.sh",
  "dev:clean": "./scripts/dev_clean.sh",
  "test:quick": "./scripts/quick_test.sh",
  "deploy:static": "export DEPLOY_BASE_PATH='/classes/863.25/people/HayleyBloch' NEXT_PUBLIC_TARGET_URL='https://fab.cba.mit.edu/classes/863.25/people/HayleyBloch/' NEXT_PUBLIC_BASE_PATH='/classes/863.25/people/HayleyBloch' && npm run build:static && GITLAB_REPO=\"ssh://git@gitlab.cba.mit.edu:846/classes/863.25/people/HayleyBloch.git\" GIT_BRANCH=\"main\" ./scripts/push_to_gitlab.sh",
  "preview:web": "npm run preview:static --workspace=apps/web",
  "fix:export-paths": "node scripts/fix_export_paths.js",
  "preview:eleventy": "export DEPLOY_BASE_PATH='/classes/863.25/people/HayleyBloch' && npm run build:eleventy && node scripts/preview_static_node.js",
  "preview:static": "npm run build:static:clean && npm run preview:eleventy",
  "prepreview:full-static": "node scripts/tools/archived/prepreview_full_static.js",
  "preview:full-static": "node scripts/tools/archived/preview_full_static.js",
  "preview:full-static:dev": "node scripts/tools/archived/preview_full_static_dev.js"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/nodemailer": "^6.4.17",
    "chokidar": "^3.5.3",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "sharp": "^0.34.4",
    "turbo": "^2.5.5",
  "@11ty/eleventy": "^2.0.1"
  },
  "engines": {
    "node": "22.x"
  },
  "packageManager": "npm@11.4.2",
  "dependencies": {
    "@types/three": "^0.180.0",
    "@xterm/xterm": "^5.5.0",
    "emulators-ui": "^0.73.9",
    "i18next": "^25.3.2",
    "next-i18next": "^15.4.2",
    "nodemailer": "^7.0.5",
    "puppeteer": "^24.23.0",
    "react-i18next": "^15.6.1",
    "three": "^0.180.0",
    "zod": "^4.0.5"
  }
}
